{
  "expo": {
    "name": "QuizGo",
    "slug": "QuizGo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/quizz-logo.png",
    "scheme": "reactnativepractice",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/quizz-logo.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gokulramt.ReactNativePractice"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/quizz-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/quizz.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-web-browser",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ef57e766-4d33-4e37-a5df-6ecf4ec9787e"
      }
    }
  }
}
